.tmpl-list {
	padding: 30rpx;
}

.tmpl-list .title {
	font-size: 28rpx;
	font-weight: bold;
	margin-bottom: 20rpx;
}

.tmpl-list .tmpl-boxs {
	display: flex;
	flex-direction: column;
    justify-content: flex-start;
    align-items: left;
}

.tmpl-list .tmpl-item {
	display: flex;
	flex-direction: row;
    justify-content: flex-start;
	align-items: center;
	margin-top: 20rpx;
}

.tmpl-list .tmpl-item-img {
	width: 40rpx;
	height: 40rpx;
	margin-right: 20rpx;	
	border-radius: 50%;
}

.tmpl-list .tmpl-item-title {
	font-size: 28rpx;
}
.btn-list {
	padding: 40rpx 40rpx;
	display: flex;
	flex-wrap: nowrap;
	justify-content: space-between;
	align-items: center;
	gap: 24rpx;
}

.btn-item {
	height: 80rpx;
	color: #ffffff;
	background-color: #1aad19;
	margin: 0;
	border-radius: 8rpx;
	white-space: nowrap;
}

.log-content {
	margin: 40rpx 40rpx;
}

.user-content {
	min-height: 800rpx;
	width: 100%;
	background-color: #e1e1e1;
	border-radius: 16rpx;
	padding: 20rpx;
	color: #515151;
}