    "eslint-plugin-prettier": "^4.0.0",
    "glob": "^10.4.5",
    "husky": "^9.1.1",
    "lint-staged": "^12.3.4",
    "miniprogram-api-typings": "^2.8.3-1",
    "prettier": "^2.2.1",
    "rimraf": "^5.0.9"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^7.16.1",
